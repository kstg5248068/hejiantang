<!--index.wxml-->
<view class="container">
  
          <swiper class="page-section page-section-spacing"
        indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" 
        interval="{{interval}}" duration="{{duration}} ">
        <block wx:for="{{imgUrls}}" wx:key="*this" >
          <swiper-item>
            <image src="{{item}}"/>
          </swiper-item>
        </block>
      </swiper>


</view>